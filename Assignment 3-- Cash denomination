 let amount = parseInt(prompt("Enter your amount: "));
let currencyNotes = [5000, 1000, 500, 100, 50, 20, 10, 5, 2, 1];


     
for (let i = 0; i < currencyNotes.length; i++){
     let currencyNote = currencyNotes[i]
     let curr = Math.floor(amount / currencyNote);
     
   console.log(`${currencyNotes[i]} notes of = ${curr}`);
   
}
     
